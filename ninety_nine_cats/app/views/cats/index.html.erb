<h2> Cats </h2>
<a href="<%= new_cat_url %>"> Create new cat </a>
<ul>
  <% @cats.each do |cat| %>
    <li><%= cat.name %></li>
    <ul>
      <li> <a href="<%= cat_url(cat) %>"> View <%= cat.name %></a> </li>
      <li> <a href="<%= edit_cat_url(cat) %>"> Cat makeover party! woo!! </a> </li>
    </ul>
  <% end %>
</ul>